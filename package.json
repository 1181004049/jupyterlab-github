{
  "name": "@jupyterlab/github",
  "version": "0.4.0",
  "description": "JupyterLab viewer for GitHub repositories",
  "keywords": [
    "github",
    "jupyter",
    "jupyterlab"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-github",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-github/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Ian Rose",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-github.git"
  },
  "scripts": {
    "build": "tsc",
    "build:test": "cd test && ./build-tests.sh",
    "clean": "rimraf lib",
    "test": "cd test && ./run-tests.sh",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.14.0",
    "@jupyterlab/apputils": "^0.14.0",
    "@jupyterlab/coreutils": "^1.0.1",
    "@jupyterlab/docmanager": "^0.14.0",
    "@jupyterlab/docregistry": "^0.14.0",
    "@jupyterlab/filebrowser": "^0.14.0",
    "@jupyterlab/observables": "^1.0.1",
    "@jupyterlab/services": "^1.0.1",
    "@phosphor/algorithm": "^1.1.2",
    "@phosphor/signaling": "^1.2.2",
    "@phosphor/widgets": "^1.5.0"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "~2.6.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
